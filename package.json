{
  "name": "react-boilerplate",
  "version": "1.0.0",
  "description": "React Boilerplate",
  "main": "src/App.jsx",
  "repository": "https://github.com/dkocjan/react-boilerplate.git",
  "author": "dkocjan",
  "license": "MIT",
  "scripts": {
    "build": "./node_modules/.bin/webpack -p --env.prod",
    "dev": "./node_modules/.bin/webpack-dev-server --env.dev",
    "format": "./node_modules/.bin/prettier --write --single-quote --print-width=120 --parser=flow --tab-width=2 **/*.{js,jsx}",
    "lint": "./node_modules/.bin/eslint **/*.{js,jsx} --quiet",
    "watch": "./node_modules/.bin/webpack --watch -env.dev",
    "start": "yarn dev"
  },
  "dependencies": {
    "react": "^16.1.1",
    "react-dom": "^16.1.1",
    "react-router-dom": "^4.2.2",
    "styled-components": "^2.2.3",
    "webpack": "^3.8.1"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.2",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.26.0",
    "eslint": "^4.12.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-config-react": "^1.1.7",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-prettier": "^2.3.1",
    "eslint-plugin-react": "^7.5.1",
    "prettier": "^1.8.2",
    "webpack-config-utils": "^2.3.0",
    "webpack-dev-server": "^2.9.4"
  }
}
